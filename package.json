{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "agent": "pnpm -C finassist-agent dev",
    "agent:build": "pnpm -C finassist-agent build",
    "agent:start": "pnpm -C finassist-agent start",
    "agent:node": "pnpm -C agent-starter-node dev",
    "agent:node:start": "pnpm -C agent-starter-node start",
    "agent:node:download": "pnpm -C agent-starter-node download-files",
    "agent:web": "pnpm -C finassist-agent dev",
    "agent:up": "concurrently -n node,web -c blue,green \"pnpm agent:node\" \"pnpm agent:web\""
  },
  "dependencies": {
    "@ai-sdk/elevenlabs": "^1.0.14",
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/openai": "^2.0.52",
    "@google-cloud/speech": "^7.2.1",
    "@livekit/agents": "^1.0.11",
    "@livekit/components-react": "^2.9.15",
    "@livekit/components-styles": "^1.1.6",
    "@livekit/rtc-node": "^0.13.20",
    "ai": "^5.0.76",
    "livekit-client": "^2.15.11",
    "livekit-server-sdk": "^2.14.0",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^11.0.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.18.3"
  ,
  "pnpm": {
    "overrides": {
      "@img/sharp-libvips-darwin-arm64": "^1.2.3",
      "@img/sharp-libvips": "^1.2.3"
    }
  }
